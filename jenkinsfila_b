pipeline {
    agent any 
    
    stages {
        stage('Job Name Script') { 
            steps { 
                sh 'python3 ../Etoro/job_name.py'
            }
        }
       
        stage('Start job2') { 
            steps { 
              build job: 'Etoro3', parameters: [[$class: 'StringParameterValue', name: 'JOB_NAME', value: env.JOB_NAME]]

            }
        }
    }
}
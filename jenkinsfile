pipeline {
    agent any 
    
    stages {
        stage('Pull from SCM') { 
            steps { 
                sh 'echo Pull from SCM'
            }
        }
        
    }

    stage('Deploy on cluster 2') {
            steps {
                script {
                     {
                        build job: 'Etoro2', parameters: [[$class: 'BooleanParameterValue', name: 'BUILD_CLUSTER_2', value: "false"]]
                    }
                }
            }
        }


}

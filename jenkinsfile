pipeline {
    agent any 
    parameters {
        string(name: 'FIRST_JOB_NAME', defaultValue: 'Etoro', description: 'first Etoro job')
    }
    
    stages {
        stage('Pull from SCM') { 
            steps { 
                sh 'echo Pull from SCM'
            }
        }


        stage('Start job2') { 
            steps { 
              build job: 'Etoro2', parameters: [[$class: 'StringParameterValue', name: 'FIRST_JOB_NAME', value: 'Etoro']]

            }
        }
        
    }


             
       


}

pipeline {
    agent any 
    
    stages {
        stage('Pull from SCM') { 
            steps { 
                sh 'echo Pull from SCM'
            }
        }


        stage('Start job2') { 
            steps { 
              build job: 'Etoro2', parameters: [[$class: 'StringParameterValue', name: 'JOB_NAME', value: env.JOB_NAME]]

            }
        }
        
    }


             
       


}

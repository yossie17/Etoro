pipeline {
    agent any 
    
    stages {
        stage('Build') { 
            steps {
                sh 'echo Building' 
                sh 'echo $BUILD_NUMBER && echo $JOB_NAME' 
                sh 'python3 job_name.py' 
            }
        }
       
        stage('Deploy') { 
            steps {
                sh 'echo Deploing' 
                 
            }
        }
    }
}

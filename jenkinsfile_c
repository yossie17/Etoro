pipeline {
    agent any 
    
    stages {
        stage('Build number of the first job') { 
            steps { 
                script{
                    // def jobname = "Etoro"
                    def jobname = env.FIRST_JOB_NAME
                    def job = Jenkins.instance.getItemByFullName(jobname)
                    def build = job.getLastBuild()
                    print(build)
                }
                
             
            }
        }
       
        
    }
}